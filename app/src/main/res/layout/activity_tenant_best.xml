                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:text="立即加入"
                        android:textColor="#ffffff"
                        android:textSize="20dp" />
                </RelativeLayout>
            </LinearLayout>
        </RelativeLayout>
    </LinearLayout>
